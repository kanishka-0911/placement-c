#include <stdio.h>

int main()
{
    int a,b,c;
    printf("Enter number:");
    scanf("%d %d %d",&a,&b,&c);
    
    if(a>b && a>c)
    {   
        printf("A IS GREATER");
    } else  if(b>a && b>c)
    {
        printf("B IS GREATER");
    } else
    {
         printf("C IS GREATER");
    }
    return 0;
}
